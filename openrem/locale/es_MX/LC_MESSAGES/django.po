#, python-brace-format
msgid ""
"studies with description that do not include '{text}' removed {num} studies, "
msgstr ""
"estudios cuya descripción no incluya '{text}' eliminaron {num} estudios, "

#: remapp/netdicom/qrscu.py:1711
#, python-brace-format
msgid ""
"only studies with station names that include '{text}' removed {num} studies, "
msgstr ""
"solo estudios con nombre de estación que incluya '{text}' eliminaron {num} "
"estudios, "

#: remapp/netdicom/qrscu.py:1718
#, python-brace-format
msgid ""
"studies with station names that do not include '{text}' removed {num} "
"studies, "
msgstr ""
"estudios con nombre de estación que no incluyan '{text}' eliminaron {num} "
"estudios, "

#: remapp/netdicom/qrscu.py:1726
#, python-brace-format
msgid ""
"Removing duplicates of previous objects removed {duplicates_removed} studies."
msgstr ""
"La eliminación de duplicados de objetos anteriores elimino estudios de "
"{duplicates_removed}."

